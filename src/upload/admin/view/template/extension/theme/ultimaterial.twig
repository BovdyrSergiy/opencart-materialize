{{ header }}{{ column_left }}
<div id="content">
	<div class="page-header">
		<div class="container-fluid">
			<div class="float-right">
				<button type="submit" form="form-theme" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary"><i class="fas fa-save"></i></button>
				<a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-light"><i class="fas fa-reply"></i></a>
			</div>
			<h1>{{ heading_title }}</h1>
			<ol class="breadcrumb">
				{% for breadcrumb in breadcrumbs %}
					<li class="breadcrumb-item"><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
				{% endfor %}
			</ol>
		</div>
	</div>
	<div class="container-fluid">
		{% if error.warning %}
			<div class="alert alert-danger alert-dismissible"><i class="fas fa-exclamation-circle"></i>
				{{ error.warning }}
				<button type="button" class="close" data-dismiss="alert">&times;</button>
			</div>
		{% endif %}
		<div class="card">
			<div class="card-header"><i class="fas fa-pencil-alt"></i> {{ text_edit }}</div>
			<div class="card-body">
				<form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-theme">
					<fieldset>
						<legend>{{ text_general }}</legend>
						<div class="mb-5">
							<div class="mdc-select mdc-select--outlined" data-mdc-auto-init="MDCSelect">
								<input type="hidden" name="theme_ultimaterial_directory" id="input-directory">
								<i class="mdc-select__dropdown-icon"></i>
								<div id="entry-directory-selected-text" class="mdc-select__selected-text" role="button" aria-haspopup="listbox" aria-labelledby="entry-directory-label entry-directory-selected-text"></div>
								<div class="mdc-select__menu mdc-menu mdc-menu-surface" role="listbox">
									<ul class="mdc-list">
										{% for directory in directories %}
											{% if directory == theme_ultimaterial_directory %}
												<li class="mdc-list-item mdc-list-item--selected" data-value="{{ directory }}" role="option" aria-selected="true">{{ directory }}</li>
											{% else %}
												<li class="mdc-list-item" data-value="{{ directory }}" role="option">{{ directory }}</li>
											{% endif %}
										{% endfor %}
									</ul>
								</div>
								<div class="mdc-notched-outline">
									<div class="mdc-notched-outline__leading"></div>
									<div class="mdc-notched-outline__notch">
										<label class="mdc-floating-label" id="entry-directory-label">{{ entry_directory }}</label>
									</div>
									<div class="mdc-notched-outline__trailing"></div>
								</div>
							</div>
							<span class="mdc-select-helper-text mdc-select-helper-text--persistent">{{ help_directory }}</span>
						</div>
						<div>
							<div class="mdc-select mdc-select--outlined" data-mdc-auto-init="MDCSelect">
								<input type="hidden" name="theme_ultimaterial_status" id="input-status">
								<i class="mdc-select__dropdown-icon"></i>
								<div id="entry-status-selected-text" class="mdc-select__selected-text" role="button" aria-haspopup="listbox" aria-labelledby="entry-directory-label entry-status-selected-text"></div>
								<div class="mdc-select__menu mdc-menu mdc-menu-surface" role="listbox">
									<ul class="mdc-list">
										{% if theme_ultimaterial_status %}
											<li class="mdc-list-item mdc-list-item--selected" data-value="1" role="option" aria-selected="true">{{ text_enabled }}</li>
											<li class="mdc-list-item" data-value="0" role="option">{{ text_disabled }}</li>
										{% else %}
											<li class="mdc-list-item" data-value="1" role="option">{{ text_enabled }}</li>
											<li class="mdc-list-item mdc-list-item--selected" data-value="0" role="option" aria-selected="true">{{ text_disabled }}</li>
										{% endif %}
									</ul>
								</div>
								<div class="mdc-notched-outline">
									<div class="mdc-notched-outline__leading"></div>
									<div class="mdc-notched-outline__notch">
										<label class="mdc-floating-label" id="entry-status-label">{{ entry_status }}</label>
									</div>
									<div class="mdc-notched-outline__trailing"></div>
								</div>
							</div>
						</div>
					</fieldset>
					<fieldset>
						<legend>{{ text_product }}</legend>
						<div class="mb-5">
