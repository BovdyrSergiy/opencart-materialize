	<script>
		$('.sidenav').sidenav();
		$('.dropdown-trigger').dropdown();

		let sliderHeaderShadow = document.getElementById('slider-header-shadow');
		noUiSlider.create(sliderHeaderShadow, {
			start: 1,
			step: 1,
			connect: [true, false],
			range: {
				'min': 0,
				'max': 5
			},
			format: wNumb({
				decimals: 0
			}),
		});

		sliderHeaderShadow.noUiSlider.on('change', function (values) {
			$('#input-header-shadow').val(values[0]).change();
		});

		let sliderImageWidth = document.getElementById('slider-card-default-image-width');
		noUiSlider.create(sliderImageWidth, {
			start: {{ theme_mt_materialize_image_product_width }},
			step: 1,
			connect: [true, false],
			range: {
				'min': 0,
				'max': 400
			},
			format: wNumb({
				decimals: 0
			}),
		});

		sliderImageWidth.noUiSlider.on('change', function (values) {
