	<main>
		<div class="container" id="mt-cards-demo">
			<h5>Default Card</h5>
			<div class="row">
				{% for i in 1..4 %}
					<div class="col-12 col-sm-6 col-lg-4 col-xl-3">
						<div class="mt-card" data-element="mt-card">
							<div class="mt-card-content scale-transition scale-out" data-change="scale-in" data-element="mt-card-default-title-position-header">
								<div class="mt-card-title__wrapper">
									<span class="mt-card__title grey-text text-darken-4" data-old-class="grey-text text-darken-4" data-element="mt-card-title">Lorem ipsum dolor sit amet</span>
									<span class="mt-card__subtitle grey-text scale-transition scale-out" data-old-class="grey-text" data-change="scale-in" data-element="mt-card-manufacturer">McDonald’s</span>
									<div class="divider scale-transition scale-out" data-change="scale-in" data-element="mt-card-divider-position-after-title"></div>
								</div>
							</div>
							<div class="mt-card-image">
								<img src="{{ mt_placeholder }}" alt="I want to eat" title="I want to eat" data-element="mt-card-default-image">
								<div class="mt-card-title__wrapper scale-transition scale-out" data-change="scale-in" data-element="mt-card-default-title-position-over-media">
									<span class="mt-card__title black-text" data-old-class="black-text" data-element="mt-card-title">Lorem ipsum dolor sit amet</span>
									<span class="mt-card__subtitle grey-text scale-transition scale-out" data-old-class="grey-text" data-change="scale-in" data-element="mt-card-manufacturer">McDonald’s</span>
									<div class="divider scale-transition scale-out" data-change="scale-in" data-element="mt-card-divider-position-after-title"></div>
								</div>
								<button class="mt-btn-floating mt-btn-large halfway-fab waves-effect waves-light red scale-transition scale-out" data-change="scale-in" data-element="mt-card-cart-floating-button"><i class="material-icons">add_shopping_cart</i></button>
							</div>
							<div class="mt-card-content">
								<div class="mt-card-title__wrapper scale-transition scale-out scale-in" data-change="scale-in" data-element="mt-card-default-title-position-default">
									<span class="mt-card__title grey-text text-darken-4" data-old-class="grey-text text-darken-4" data-element="mt-card-title">Lorem ipsum dolor sit amet</span>
									<span class="mt-card__subtitle grey-text scale-transition scale-out" data-old-class="grey-text" data-change="scale-in" data-element="mt-card-manufacturer">McDonald’s</span>
									<div class="divider scale-transition scale-out" data-change="scale-in" data-element="mt-card-divider-position-after-title"></div>
								</div>
								<div class="mt-card-price">
									{% if loop.index is odd %}
										<span class="mt-card-price__item mt-card-price__item--old-price grey-text" data-element="mt-card-old-price">{{ price }}</span>
										<span class="mt-card-price__item mt-card-price__item--new-price red-text text-darken-2" data-element="mt-card-new-price">{{ price }}</span>
									{% else %}
										<span class="mt-card-price__item mt-card-price__item--current-price" data-element="mt-card-current-price">{{ price }}</span>
									{% endif %}
								</div>
