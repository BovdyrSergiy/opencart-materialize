{% if mt_filters %}
	<h4 class="center">{{ php_time }} millisecond</h4>
	<div id="mt-filter-category">
		<ul class="collection with-header mt-filter{{ horizontal ? ' horizontal' : ' collapsible expandable' }} z-depth-1 {{ color.common.background.name }} {{ color.common.text.name }}">
			<li class="collection-header mt-filters-item {{ color.header.background.name }} {{ color.header.text.name }}"><h4>{{ module_name }}</h4></li>
			{% for mt_filter in mt_filters %}
				{% for name, inputs_data in mt_filter.value %}
					<li class="collection-item mt-filters-item arrow-rotate{{ mt_filter.collapsible ? ' active' }}">
						<div class="mt-filter-title collapsible-header waves-effect {{ color.header.background.name }} {{ color.header.text.name }}"><b>{{ name }}</b></div>
						<div class="mt-filter-content collapsible-body"{% if mt_filter.type != 'select_single' and mt_filter.type != 'select_multiple' %} data-simplebar{% endif %}>
							{% if mt_filter.input_type == 'select_single' or mt_filter.type == 'select_multiple' %}
								<div class="input-field">
									<select name="{{ inputs_data.input_name }}"{% if mt_filter.type == 'select_multiple' %} multiple{% endif %}>
										<option value=""{% if mt_filter.type == 'select_multiple' %} disabled{% endif %}>{{ text_select }}</option>
										{% for input_value in inputs_data.input_values %}
											<option value="{{ input_value.value }}" id="{{ input_value.mt_filter_id }}"{% if input_value.image %} data-icon="{{ input_value.image }}"{% endif %}{{ input_value.preselected ? ' checked="checked"' }}{{ input_value.product_count_disabled ? ' disabled' }}>{{ input_value.name }}{% if input_value.product_count is not null %} ({{ input_value.product_count }}){% endif %}</option>
										{% endfor %}
									</select>
								</div>
							{% else %}
								<ul>
									{% for input_value in inputs_data.input_values %}
										<li class="mt-filter-item">
											{% if mt_filter.input_type == 'checkbox' %}
												<label class="mt-filter-item--checkbox">
													{% if input_value.image %}<img src="{{ input_value.image }}" alt="{{ input_value.name }}">{% endif %}
													<input type="checkbox" name="{{ inputs_data.input_name }}" value="{{ input_value.value }}" id="{{ input_value.mt_filter_id }}" class="filled-in"{{ input_value.preselected ? ' checked="checked"' }}{{ input_value.product_count_disabled ? ' disabled' }}>
													<span>{{ input_value.name }}{% if input_value.product_count is not null %} ({{ input_value.product_count }}){% endif %}</span>
												</label>
											{% endif %}
											{#{% if mt_filter.input_type == 'radio' %}
												<label class="mt-filter-item--radio">
													{% if value.image %}<img src="{{ value.image }}" alt="{{ value.name }}">{% endif %}
													<input type="radio" name="{{ value.input_name }}" value="{{ value.filter_value }}" id="{{ value.mt_filter_id }}" class="with-gap"{{ value.preselected ? ' checked="checked"' }}>
													<span>{{ value.name }}</span>
												</label>
											{% endif %}
											{% if mt_filter.input_type == 'switches' %}
												<div class="switch">
													<label class="mt-filter-item--switch">
														{% if value.image %}<img src="{{ value.image }}" alt="{{ value.name }}">{% endif %} #}{# todo-materialize To set styles for images #}{#
														{{ value.name }} #}{# todo-materialize Set styles for all "switches" so that they are on the right #}{#
														<input type="checkbox" name="{{ value.input_name }}" value="{{ value.filter_value }}" id="{{ value.mt_filter_id }}"{{ value.preselected ? ' checked="checked"' }}>
														<span class="lever"></span>
													</label>
												</div>
											{% endif %}#}
										</li>
									{% endfor %}
								</ul>
							{% endif %}
						</div>
					</li>
				{% endfor %}
				{#{% if mt_filter.type == 'price' and mt_filter.value %}
					<li class="collection-item mt-filters-item arrow-rotate{{ mt_filter.collapsible ? ' active' }}"> #}{# todo-materiliaze Set color from color scheme #}{#
						<div class="mt-filter-title collapsible-header waves-effect {{ color.header.background.name }} {{ color.header.text.name }}"><span><b>{{ mt_filter.name }} <span id="span-price-range-min">{{ prices.min_price }}</span> â€” <span id="span-price-range-max">{{ prices.max_price }}</span></b></span></div>
						<div class="mt-filter-content collapsible-body">
							<div id="slider-prices" class="mt-filters-item--filter-sliders {{ color.slider.secondary_background.name }}"></div>
							<input type="hidden" name="price_filter[]" value="{{ price_filter_preselected.0 }}" id="input-price-range-min">
							<input type="hidden" name="price_filter[]" value="{{ price_filter_preselected.1 }}" id="input-price-range-max">
						</div>
					</li>
				{% endif %}
				{% if mt_filter.type == 'keyword' %}
